{
  "title": "ASP.NET Core Vkontakte middleware",
  "version": "1.0.0",
  "description": "ASP.NET Core middleware that enables an application to support Vkontakte OAuth 2.0 authentication workflow.",
  "authors": [ "Albert Zakiev" ],

  "packOptions": {
    "projectUrl": "https://github.com/serber/Brik.Security.VkontakteMiddleware",
    "licenseUrl": "https://github.com/serber/Brik.Security.VkontakteMiddleware/blob/master/LICENSE",
    "tags": [ "aspnetcore", "authentication", "security", "vk", "vkontakte" ],
    "requireLicenseAcceptance": false
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "nowarn": [ "CS1591" ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Authentication.OAuth": "1.0.0",
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "net451": {},

    "netstandard1.3": {
      "dependencies": {
        "System.Dynamic.Runtime": "4.0.11"
      },

      "imports": [
        "dotnet5.4",
        "portable-net451+win8"
      ]
    }
  }
}